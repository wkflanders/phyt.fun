{
    "$schema": "https://turbo.build/schema.v2.json",
    "tasks": {
        "prebuild": {
            "outputs": [],
            "cache": false
        },
        "build": {
            "dependsOn": ["prebuild", "^build"],
            "outputs": [".next/**", "dist/**", "build/**", "src/**/*.json"],
            "env": [
                "NEXT_PUBLIC_*",
                "PRIVY_APP_ID",
                "PRIVY_SECRET_KEY",
                "PRIVY_VERIFICATION_KEY",
                "API_URL",
                "DATABASE_URL",
                "BASE_RPC_URL",
                "EXCHANGE_ADDRESS",
                "PHYT_CARDS_ADDRESS",
                "EXECUTOR_ADDRESS",
                "MINTER_ADDRESS",
                "SERVER_ADDRESS",
                "SERVER_PRIVATE_KEY",
                "SERVER_PORT",
                "ADMIN_IDS",
                "POSTGRES_HOST",
                "POSTGRES_PORT",
                "POSTGRES_USER",
                "POSTGRES_PASSWORD",
                "POSTGRES_DB",
                "POSTGRES_URL",
                "FRONTEND_URL",
                "AWS_REGION",
                "AWS_ACCESS_KEY",
                "AWS_SECRET_ACCESS_KEY",
                "AWS_IMAGES_BUCKET",
                "AWS_CLOUDFRONT_IMAGES_URL",
                "AWS_METADATA_BUCKET",
                "AWS_CLOUDFRONT_METADATA_URL",
                "AWS_AVATAR_URL_BUCKET",
                "AWS_CLOUDFRONT_AVATAR_URL",
                "AWS_CLOUDFRONT_AVATAR_URL_KEY_ID",
                "AWS_CLOUDFRONT_AVATAR_URL_PRIVATE_KEY",
                "NODE_ENV"
            ]
        },
        "@phyt/eslint#build": {
            "outputs": ["dist/**"]
        },
        "@phyt/types#build": {
            "dependsOn": ["@phyt/eslint#build"],
            "outputs": ["dist/**"]
        },
        "@phyt/models#build": {
            "dependsOn": ["@phyt/eslint#build"],
            "outputs": ["dist/**"]
        },
        "@phyt/repositories#build": {
            "dependsOn": ["@phyt/models#build"],
            "outputs": ["dist/**"]
        },
        "@phyt/dto#build": {
            "dependsOn": ["@phyt/models#build"],
            "outputs": ["dist/**"]
        },
        "@phyt/drizzle#build": {
            "dependsOn": [
                "@phyt/models#build",
                "@phyt/repositories#build",
                "@phyt/dto#build"
            ],
            "outputs": ["dist/**"]
        },
        "@phyt/server#build": {
            "dependsOn": ["@phyt/drizzle#build", "@phyt/contracts#build"],
            "outputs": ["dist/**"]
        },
        "@phyt/web#build": {
            "dependsOn": ["@phyt/drizzle#build", "@phyt/server#build"],
            "outputs": [".next/**"]
        },
        "type-check": {
            "dependsOn": ["^type-check"]
        },
        "lint": {
            "dependsOn": ["^lint"]
        },
        "lint:fix": {
            "dependsOn": ["^lint:fix"]
        },
        "@phyt/types#lint:fix": {
            "dependsOn": ["@phyt/eslint#lint:fix"]
        },
        "@phyt/models#lint:fix": {
            "dependsOn": ["@phyt/eslint#lint:fix"]
        },
        "@phyt/repositories#lint:fix": {
            "dependsOn": ["@phyt/models#build"]
        },
        "@phyt/dto#lint:fix": {
            "dependsOn": ["@phyt/models#lint:fix"]
        },
        "@phyt/drizzle#lint:fix": {
            "dependsOn": [
                "@phyt/models#lint:fix",
                "@phyt/repositories#lint:fix",
                "@phyt/dto#lint:fix"
            ]
        },
        "@phyt/web#lint:fix": {
            "dependsOn": ["@phyt/drizzle#lint:fix", "@phyt/contracts#lint:fix"]
        },
        "@phyt/server#lint:fix": {
            "dependsOn": ["@phyt/drizzle#lint:fix", "@phyt/web#lint:fix"]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "clean": {
            "cache": false
        }
    }
}
