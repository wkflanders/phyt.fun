{
    "name": "@phyt/infra",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "types": "./src/index.ts",
    "main": "./dist/index.js",
    "module": "./dist/esm/index.js",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./dist/index.js",
            "require": "./dist/cjs/index.cjs"
        },
        "./server": {
            "types": "./src/server.ts",
            "import": "./dist/server.js",
            "require": "./dist/cjs/server.cjs"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts src/server.ts --dts --format esm,cjs --out-dir dist",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "update": "pnpm update --latest",
        "clean": "rm -rf dist .turbo node_modules"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.820.0",
        "@privy-io/server-auth": "1.19.2",
        "@t3-oss/env-core": "^0.13.6",
        "dotenv": "^16.5.0",
        "uuid": "^11.1.0",
        "viem": "^2.30.5",
        "zod": "^3.25.42"
    },
    "devDependencies": {
        "@phyt/eslint": "workspace:*",
        "@phyt/tsconfig": "workspace:*",
        "@phyt/types": "workspace:*"
    }
}
