{
    "name": "@phyt/services",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "types": "./src/index.ts",
    "main": "./dist/index.js",
    "module": "./dist/esm/index.js",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./dist/index.js",
            "require": "./dist/cjs/index.cjs"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "type-check": "tsc --noEmit",
        "update": "pnpm update --latest",
        "clean": "rm -rf dist .turbo node_modules"
    },
    "dependencies": {
        "@phyt/aws": "workspace:*",
        "@phyt/contracts": "workspace:*",
        "@phyt/infra": "workspace:*",
        "@phyt/models": "workspace:*",
        "merkletreejs": "^0.5.2",
        "viem": "^2.30.5"
    },
    "devDependencies": {
        "@phyt/dto": "workspace:*",
        "@phyt/eslint": "workspace:*",
        "@phyt/repositories": "workspace:*",
        "@phyt/test": "workspace:*",
        "@phyt/tsconfig": "workspace:*",
        "@phyt/types": "workspace:*",
        "vitest": "^3.2.1",
        "zod": "^3.25.42"
    }
}
