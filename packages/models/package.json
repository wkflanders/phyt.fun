{
    "name": "@phyt/models",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "types": "./src/index.ts",
    "main": "./dist/index.js",
    "module": "./dist/esm/index.js",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./dist/index.js",
            "require": "./dist/cjs/index.cjs"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "update": "pnpm update --latest",
        "clean": "rm -rf dist .turbo node_modules",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:verbose": "vitest run --reporter=verbose",
        "test:debug": "vitest run --reporter=verbose --no-coverage",
        "test:users": "vitest run src/__tests__/usersModel.test.ts --reporter=verbose",
        "test:single": "vitest run --reporter=verbose -t",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@phyt/types": "workspace:*",
        "zod": "^3.25.42"
    },
    "devDependencies": {
        "@phyt/eslint": "workspace:*",
        "@phyt/infra": "workspace:*",
        "@phyt/tsconfig": "workspace:*",
        "@phyt/test": "workspace:*",
        "vitest": "^2.1.8"
    }
}
