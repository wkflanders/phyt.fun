{
    "name": "@phyt/contracts",
    "version": "0.0.0",
    "private": true,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "forge build && tsx script/export-abis.ts && tsc -p tsconfig.json",
        "export-abis": "tsx script/export-abis.ts",
        "clean": "rm -rf dist .turbo node_modules ./src/*.json ./src/index.ts ./out"
    },
    "devDependencies": {
        "@phyt/tsconfig": "workspace:*",
        "@types/node": "^20.17.28",
        "tsx": "^4.19.3",
        "typescript": "^5.8.2"
    }
}
