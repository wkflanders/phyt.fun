{
    "name": "@phyt/drizzle",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "types": "./src/index.ts",
    "main": "./dist/cjs/index.cjs",
    "module": "./dist/esm/index.js",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.cjs"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "update": "pnpm update --latest",
        "clean": "rm -rf dist .turbo node_modules",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:migrate-reset": "drizzle-kit migrate:reset",
        "db:push": "drizzle-kit push",
        "db:reset": "node --loader ts-node/esm src/reset.ts",
        "db:studio": "drizzle-kit studio",
        "db:seed": "node --loader ts-node/esm src/seed.ts"
    },
    "dependencies": {
        "@phyt/models": "workspace:*",
        "@t3-oss/env-core": "^0.13.6",
        "dotenv": "^16.5.0",
        "drizzle-orm": "^0.44.0",
        "pg": "^8.16.0",
        "uuid": "^11.1.0",
        "zod": "^3.25.42"
    },
    "devDependencies": {
        "@phyt/eslint": "workspace:*",
        "@phyt/tsconfig": "workspace:*",
        "@phyt/types": "workspace:*",
        "@types/pg": "^8.15.2",
        "drizzle-kit": "^0.31.1"
    }
}
