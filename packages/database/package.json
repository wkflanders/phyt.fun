{
    "name": "@phyt/database",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "types": "./src/index.ts",
    "main": "./dist/cjs/index.cjs",
    "module": "./dist/esm/index.js",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.cjs"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "clean": "rm -rf dist .turbo node_modules",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit push",
        "db:reset": "ts-node src/reset.ts",
        "db:studio": "drizzle-kit studio",
        "db:seed": "ts-node src/seed.ts"
    },
    "dependencies": {
        "@phyt/eslint": "workspace:*",
        "@t3-oss/env-core": "^0.12.0",
        "drizzle-orm": "^0.39.3",
        "pg": "^8.14.1",
        "uuid": "^11.1.0",
        "ws": "^8.18.1",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@phyt/tsconfig": "workspace:*",
        "@types/pg": "^8.11.11",
        "@types/ws": "^8.18.0",
        "drizzle-kit": "^0.30.6",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.2"
    }
}
