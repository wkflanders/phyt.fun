{
    "name": "@phyt/test",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "types": "./dist/src/index.d.ts",
    "main": "./dist/src/index.js",
    "module": "./dist/src/index.js",
    "exports": {
        ".": {
            "types": "./dist/src/index.d.ts",
            "import": "./dist/src/index.js",
            "require": "./dist/src/index.cjs"
        },
        "./vitest.config": {
            "types": "./dist/vitest.config.d.ts",
            "import": "./dist/vitest.config.js",
            "require": "./dist/vitest.config.cjs"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts src/setup.ts vitest.config.ts --dts --format esm,cjs --out-dir dist",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "update": "pnpm update --latest",
        "clean": "rm -rf dist .turbo node_modules coverage"
    },
    "dependencies": {
        "@phyt/eslint": "workspace:*",
        "vitest": "^2.1.8",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/dom": "^10.4.0",
        "msw": "^2.8.0",
        "happy-dom": "^15.11.7"
    },
    "devDependencies": {
        "@phyt/tsconfig": "workspace:*",
        "@types/node": "^22.14.0"
    }
}
