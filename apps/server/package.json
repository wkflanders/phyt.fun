    "eslint": "^9.0.0",
    "eslint-config-prettier": ">=9.0.0",
    "eslint-import-resolver-typescript": ">=3.0.0",
    "eslint-plugin-import": ">=2.28.0",
    "eslint-plugin-node": ">=11.1.0",
