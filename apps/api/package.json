{
    "name": "@phyt/api",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "nodemon",
        "build": "tsc",
        "start": "node src/index.ts",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "type-check": "tsc --noEmit",
        "update": "pnpm update --latest",
        "update-role": "ts-node ./src/scripts/updateRole.ts",
        "clean": "rm -rf .turbo node_modules"
    },
    "dependencies": {
        "@phyt/aws": "workspace:*",
        "@phyt/drizzle": "workspace:*",
        "@phyt/dto": "workspace:*",
        "@phyt/infra": "workspace:*",
        "@phyt/models": "workspace:*",
        "@phyt/repositories": "workspace:*",
        "@phyt/services": "workspace:*",
        "@privy-io/server-auth": "^1.24.0",
        "@t3-oss/env-core": "^0.13.6",
        "@types/express-serve-static-core": "^5.0.6",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "date-fns": "^4.1.0",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "express-rate-limit": "^7.5.0",
        "express-validator": "^7.2.1",
        "helmet": "^8.1.0",
        "merkletreejs": "^0.5.2",
        "morgan": "^1.10.0",
        "multer": "2.0.0",
        "uuid": "^11.1.0",
