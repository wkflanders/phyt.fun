        "@phyt/tsconfig": "workspace:*",
        "@phyt/types": "workspace:*",
        "@types/cookie-parser": "^1.4.8",
        "@types/cors": "^2.8.18",
        "@types/express": "^5.0.2",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.15.28",
        "nodemon": "^3.1.10",
        "ts-node": "^10.9.2"
    }
